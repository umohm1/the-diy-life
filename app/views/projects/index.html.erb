<div class="container">
    <div id="otherProjects">
  <% if params[:other_users] %>
    <center><h1>New Projects!</h1></center>
  <% else %>
  <center><h1>Your Projects!</h1></center>
  <% end %>
  <div class="row">
    <div class="col-md-6">
      <% @projects.each do |project| %>
        <%= link_to project.name, user_project_path(current_user, project)%>
        <div class="show-project">
            <img class="projectImage" src="<%= project.image.url(:medium) %>" ><br><br>
        </div>
      <% end %>
    </div>
  </div>
 </div>
</div>
