<div class="container">
  <center><h1>Your Profile Page!</h1></center>
  <div class="col-md-10">
    <h3>Welcome, <%= @user.name %>!</h3>


    <div class="col-md-6">
      <% @user.projects.each do |project| %>
        <%= link_to project.name, project_path %>
        <div class="show-project">
            <img class="projectImage" src="<%= project.image.url(:medium) %>" ><br><br>
        </div>
      <% end %>

      <!-- <%#= User.most_projects.email %> has created the most projects. -->

    </div>
  </div>
</div>
